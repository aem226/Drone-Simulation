<mujoco model="Skydio X2 scene">
  <compiler angle="radian" meshdir="assets/" texturedir="assets/"/>

  <option timestep="0.01" density="1.225" viscosity="1.8e-05"/>

  <size nkey="1"/>

  <visual>
    <global azimuth="-20" elevation="-20" ellipsoidinertia="true"/>
    <headlight ambient="0.3 0.3 0.3" diffuse="0.8 0.8 0.8" specular="0 0 0"/>
    <rgba haze="0.15 0.25 0.35 1"/>
  </visual>

  <statistic meansize="0.05" extent="0.6" center="0 0 0.1"/>

  <default>
    <default class="x2">
      <mesh scale="0.01 0.01 0.01"/>
      <site group="5"/>
      <general ctrlrange="0 13"/>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
      <default class="collision">
        <geom type="box" group="3"/>
        <default class="rotor">
          <geom size="0.13 0.13 0.01" type="ellipsoid"/>
        </default>
      </default>
    </default>
  </default>

  <asset>
    <texture type="2d" colorspace="linear" name="X2_lowpoly_texture_SpinningProps_1024" file="X2_lowpoly_texture_SpinningProps_1024.png"/>
    <texture type="skybox" colorspace="auto" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
